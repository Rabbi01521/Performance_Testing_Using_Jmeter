            <stringProp name="script">import org.apache.commons.lang3.RandomStringUtils;
import java.util.Random;
import java.text.SimpleDateFormat;
import java.util.Date;

// Set amount variable to a random number between 50 and 100
int amount = new Random().nextInt(51) + 50;
vars.put(&quot;amount&quot;, String.valueOf(amount));

// Set checkIn and checkOut variables to today&apos;s date and the date four days from now, respectively
SimpleDateFormat sdf = new SimpleDateFormat(&quot;yyyy-MM-dd&quot;);
String checkIn = sdf.format(new Date());
String checkOut = sdf.format(new Date(System.currentTimeMillis() + (4 * 24 * 60 * 60 * 1000)));
vars.put(&quot;checkIn&quot;, checkIn);
vars.put(&quot;checkOut&quot;, checkOut);

// Set firstName, lastName, and depositPaid variables to random values
String firstName = RandomStringUtils.randomAlphabetic(10);
String lastName = RandomStringUtils.randomAlphabetic(10);
String depositPaidStr = String.valueOf(new Random().nextBoolean());
boolean depositPaid = Boolean.parseBoolean(depositPaidStr);
vars.put(&quot;firstName&quot;, firstName);
vars.put(&quot;lastName&quot;, lastName);
vars.put(&quot;depositPaid&quot;, depositPaidStr);

// Set randomMeal variable to a random meal from the list
String[] meals = {&quot;breakfast&quot;, &quot;lunch&quot;, &quot;dinner&quot;};
String randomMeal = meals[new Random().nextInt(meals.length)];
vars.put(&quot;randomMeal&quot;, randomMeal);

</stringProp>
